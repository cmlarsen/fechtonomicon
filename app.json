{
  "expo": {
    "name": "fechtonomicon",
    "slug": "fechtonomicon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "assetBundlePatterns": ["assets/**/*"],
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#F8F4E8"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hemaflashcards.app",
      "displayName": "The Fechto",
      "infoPlist": {
        "CFBundleDisplayName": "The Fechto",
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": true
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F8F4E8"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "single",
      "name": "Fechtonomicon",
      "shortName": "Fechtonomicon",
      "description": "Study Historical European Martial Arts with interactive flashcards",
      "backgroundColor": "#F8F4E8",
      "themeColor": "#5C3D2E"
    }
  }
}
